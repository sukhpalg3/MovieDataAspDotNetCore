@model MovieGuide.Models.Movie

@{
    ViewData["Title"] = "MovieDetails";
    var all_photos = Model.Photos;
}
<div class="jumbotron">
    <h1>@Model.MovieName ( @Model.con.GenreName ) </h1>
    <p>@Model.Description</p>
</div>
<h1>Gallery</h1>
<div class="card-columns">
    @foreach (var photo in all_photos)
    {
        <div class="card" style="height:200px">
            <img src="~/Movies/@photo.PhotoID@photo.ExtName" style="width:100%;height:100%" />
        </div>
    }

</div>